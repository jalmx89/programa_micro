#include <motor_pap.h>

#use fast_io(D)

const int8 positions[] = {0x01, 0x05, 0x04, 0x06};
const int8 positions2[] = {0x01, 0x02, 0x04, 0x08};

void main()
{
   set_tris_d(0);
   output_d(0);
   
   while(TRUE)
   {
      for(int x=0; x < 4; x++){
         output_d(positions2[x]);
         delay_ms(100);
      }
      
   }

}
