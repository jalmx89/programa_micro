#include <motor_pap.h>

#use fast_io(D)

const byte forward[] = {0x01, 0x02, 0x04, 0x08 };
const byte full[] = {0x03, 0x06, 0x0A, 0x09 };

void main()
{
   set_tris_d(0);
   output_d(0);
   
   while(TRUE)
   {
      for(int x=0; x < 4; x++){
         output_d(full[x]);
         delay_ms(100);
      }
      
   }

}
